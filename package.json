{
  "name": "gympass",
  "version": "1.0.0",
  "description": "Esta API permite o gerenciamento de check-ins de usuários em academias, oferecendo funcionalidades para usuários e administradores.",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch --env-file=.env src/server.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "start": "node --env-file=.env build/server.js",
    "build": "tsup src --out-dir build"
  },
  "keywords": [
    "api",
    "nodejs"
  ],
  "author": "Emanoel Antonio",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/bcryptjs": "2.4.6",
    "@types/node": "^22.5.2",
    "@vitest/coverage-v8": "2.1.1",
    "@vitest/ui": "2.1.1",
    "prisma": "5.19.1",
    "tsup": "^8.2.4",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "vite-tsconfig-paths": "5.0.1",
    "vitest": "2.1.1"
  },
  "dependencies": {
    "@prisma/client": "5.19.1",
    "bcryptjs": "2.4.3",
    "fastify": "^4.28.1",
    "zod": "3.23.8"
  }
}
